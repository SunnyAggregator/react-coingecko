{
  "name": "@sunnyag/react-coingecko",
  "version": "3.0.0",
  "description": "Fetch CoinGecko prices in React apps",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:SunnyAggregator/react-coingecko.git",
  "author": "Surya Khosla <surya@sunny.ag>",
  "homepage": "https://sunny.ag",
  "license": "LGPL-3.0",
  "keywords": [
    "coingecko",
    "react",
    "solana",
    "cryptocurrency",
    "sunny"
  ],
  "bugs": {
    "url": "https://github.com/SunnyAggregator/react-coingecko/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@saberhq/eslint-config": "^1.13.10",
    "@saberhq/tsconfig": "^2.0.5",
    "@types/prettier": "^2.6.1",
    "@types/react": "^17.0.45",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-query": "^3.39.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-query": "^3.39.0"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "scripts": {
    "build": "tsc && tsc -P tsconfig.esm.json",
    "clean": "rm -fr dist/",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ src/index.ts",
    "prepare": "husky install"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "packageManager": "yarn@3.2.1",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,json,jsx,html,css,md}": "prettier --write"
  }
}
